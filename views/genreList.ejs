<%- include ("layout/_head.ejs",{ title:title }) %>
    <h1>
        <%= title %>
    </h1>
    <div>
        <a href="/catalog/create-genre">Add New Genre</a>
    </div>
    <p>Following genre's are present in this Anime Section</p>


    <% if (genreList) { %>
        <ul>
            <% genreList.forEach(genre=> { %>
                <li>
                    <a href=<%=`genre/${genre.id}` %>><%= genre.genre %></a>
                </li>
                <% }) %>
        </ul>
        <% } else { %>
            <p>No Genre's  Found</p>
        <%} %>





<%- include ("layout/_footer.ejs") %>